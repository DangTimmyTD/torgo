<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="text/x-logo">
      <comment>Torgo Logo Script File</comment>
      <!-- <magic priority="50">
        <match value="search-string" type="string" offset="10:140"/>
      </magic> -->
      <sub-class-of type="text/plain"/>
      <glob pattern="*.logo"/>
      <icon name="torgo"/>
    </mime-type>
    <mime-type type="text/x-lisp">
      <comment>Torgo Lisp Script File</comment>
      <!-- <magic priority="50">
        <match value="search-string" type="string" offset="10:140"/>
      </magic> -->
      <sub-class-of type="text/plain"/>
      <glob pattern="*.lisp"/>
      <icon name="torgo"/>
    </mime-type>
    <mime-type type="text/x-bas">
      <comment>Torgo Basic Script File</comment>
      <!-- <magic priority="50">
        <match value="search-string" type="string" offset="10:140"/>
      </magic> -->
      <sub-class-of type="text/plain"/>
      <glob pattern="*.bas"/>
      <icon name="torgo"/>
    </mime-type>
</mime-info>
